{
  "name": "grocery_list",
  "version": "1.0.0",
  "description": "Add/remove/update/check/uncheck grocery list items synced in real time across multiple devices",
  "main": "src/server.js",
  "scripts": {
    "test": "set NODE_ENV=test&& jasmine",
    "start": "node src/server.js",
    "client": "cd client && npm run start",
    "server": "nodemon src/server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnreine/grocery_list.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bnreine/grocery_list/issues"
  },
  "homepage": "https://github.com/bnreine/grocery_list#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jasmine": "^3.3.1",
    "request": "^2.88.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "ejs": "^2.5.7",
    "express": "^4.16.4",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0"
  }
}
